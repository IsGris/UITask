.dropdown {
    font-size: 12px;
    color: rgb(130, 131, 147);
    -unity-text-align: middle-left;
    white-space: nowrap;
    height: 49px;
    margin-bottom: -19px;
}

.dropdown-show {
    background-image: url("project://database/Assets/Sprites/Extra/Default/input_square.png?fileID=-62245763954740697&guid=19a23c664a5dea547a985d534df8b89e&type=3#input_square_0");
    -unity-slice-left: 19;
    -unity-slice-right: 19;
    -unity-slice-top: 19;
    -unity-slice-bottom: 19;
    height: 30px;
    min-height: 30px;
    max-height: 30px;
    width: 100%;
    align-items: center;
    flex-direction: row;
    -unity-background-image-tint-color: rgb(255,255,255);
    transition-property: -unity-background-image-tint-color;
    transition-duration: 0.3s;
}
.dropdown-show:hover {
    -unity-background-image-tint-color: rgba(228, 228, 228, 1);
}

.dropdown-show-text-container {
    flex-grow: 1;
    justify-content: center;
    height: 100%;
}

.dropdown-show-text {
    margin-left: 16px;
}

.dropdown-show-icon-container {
    width: 32px;
    min-width: 32px;
    max-width: 32px;
    height: 19px;
    min-height: 19px;
    max-height: 19px;
    margin-right: 8px;
    margin-bottom: 1px;
    background-image: url("project://database/Assets/Sprites/Grey/Default/button_rectangle_depth_flat.png?fileID=-7336049532282260110&guid=9fbcef6e5bd3b704a97d9d2c956ae3c4&type=3#button_rectangle_depth_flat_0");
    -unity-slice-left: 46;
    -unity-slice-right: 46;
    -unity-slice-top: 46;
    -unity-slice-bottom: 46;
    -unity-background-image-tint-color: rgb(255,255,255);
    transition-property: -unity-background-image-tint-color;
    transition-duration: 0.3s;
}

.dropdown-show-icon-container:hover {
    -unity-background-image-tint-color: rgba(207, 207, 207, 1);
}

.dropdown-show-icon {
    flex-grow: 1;
    background-image: url("project://database/Assets/Sprites/Extra/Default/icon_arrow_up_light.png?fileID=6178840499434421544&guid=f6cdf82bf4182ea47a990e05f4ac06f2&type=3#icon_arrow_up_light_0");
    background-size: 8.5px 5.5px;
    -unity-background-image-tint-color: rgb(100, 101, 123);
    transition-property: max-height;
    transition-duration: 0.3s;
}

.dropdown-scroll-container:enabled {
    overflow: hidden;
    position: absolute;
    width: 100%;
    height: 100%;
    margin-top: 34px;
    opacity: 1;
    transition-property: opacity;
    transition-duration: 0.3s;
}

.dropdown-scroll-container:disabled {
    opacity: 0;
}

.dropdown-scroll-container:disabled .dropdown-mask-container {
    padding-top:0px;
    padding-bottom:0px;
    padding-left:0px;
    padding-right:0px;
}

.dropdown-mask-container {
    padding-top: 4px;
    padding-bottom: 4px;
    padding-left: 4px;
    padding-right: 4px;
    background-image: url("project://database/Assets/Sprites/Extra/Default/input_square.png?fileID=-62245763954740697&guid=19a23c664a5dea547a985d534df8b89e&type=3#input_square_0");
    -unity-slice-left: 19;
    -unity-slice-right: 19;
    -unity-slice-top: 19;
    -unity-slice-bottom: 19;
}

.dropdown-mask {
    overflow: hidden;
}

.dropdown-element {
    height: 25px;
    min-height: 25px;
    max-height: 25px;
    background-color: rgba(255, 255, 255, 1);
    transition-property: background-color;
    transition-duration: 0.3s;
}

.dropdown-element:hover {
    background-color: rgba(228, 228, 228, 1);
}

.dropdown-element-text {
    flex-grow: 1;
    margin-left: 12px;
}

.dropdown-element-divider {
    height: 2px;
    min-height: 2px;
    max-height: 2px;
    background-image: url("project://database/Assets/Sprites/Extra/Default/divider_no_white.png");
}

.dropdown .elements-container {
    top: 0;
}

.dropdown .dropdown-show-icon {
    transition-property: rotate;
    transition-duration: 0.3s;
    rotate: 0deg;
}

.dropdown .dropdown-show-icon.close {
    rotate: 180deg;
}
